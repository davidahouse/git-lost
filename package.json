{
  "name": "@davidahouse/git-lost",
  "main": "bin/git-lost.js",
  "version": "0.6.0",
  "description": "A command line tool for finding & managing dirty git repositories",
  "license": "MIT",
  "repository": "davidahouse/git-lost",
  "preferGlobal": true,
  "bin": {
    "git-lost": "bin/git-lost.js"
  },
  "dependencies": {
    "better-queue": "^3.8.10",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^2.20.3",
    "figlet": "^1.2.1",
    "pkginfo": "^0.4.1",
    "rc": "^1.2.8",
    "simple-git": "^1.126.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.11.0",
    "eslint-config-prettier": "^6.11.0"
  },
  "scripts": {
    "lint": "eslint $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")",
    "lint-fix": "eslint --fix $(find . -name \"*.js\" -not -path \"./node_modules/*\" -not -path \"./public/*\")"
  }
}
